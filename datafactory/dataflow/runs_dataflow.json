{
	"name": "runs_dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dt_source_csv",
						"type": "DatasetReference"
					},
					"name": "runsdataflow"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dt_sink_parquet",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id_run as short,",
				"          id_pozo as short,",
				"          {RUN_Nro.} as short,",
				"          RUN_DATE as date,",
				"          INTAKE as short,",
				"          SBHP as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> runsdataflow",
				"runsdataflow select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 cast(output(",
				"          id_run as integer,",
				"          id_pozo as integer,",
				"          {RUN_Nro.} as integer,",
				"          RUN_DATE as date,",
				"          INTAKE as double,",
				"          SBHP as double",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     partitionFileNames:['runs.pozos'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}